    </Resource>
    <Resource Include="Images\delivery.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
    </Resource>
    <Resource Include="Images\employee.png">
      <CopyToOutputDirectory>PreserveNewest</CopyToOutputDirectory>
